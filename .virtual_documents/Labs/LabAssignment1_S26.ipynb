E = 10 #Energy in eV
m = 511000 #Mass in eV/c^2
h = 4.136*(10**(-15))/(2*math.pi) #Planck's constant in eV*s
V = int(input("Please input an integer less than 10")) #step height eV
k1 = math.sqrt(2*m*E)/h
k2 = math.sqrt(2*m*(E-V))/h
T = (4*k1*k2)/((k1+k2)**2)
R = ((k1-k2)/(k1+k2))**2
print("Probability of transmission is", T*100, "% and probability of reflection is", R*10, "%")











#inputs
m = float(input("Mass of object in kg"))
F1 = float(input("Magnitude of first force on object in N"))
theta1 = float(input("Direction of first force on object in degrees"))
F2 = float(input("Magnitude of second force on object in N"))
theta2 = float(input("Direction of second force on object in degrees"))
#calculate net force and direction
theta1r = math.radians(theta1)
theta2r = math.radians(theta2)
y1 = F1*math.sin(theta1r)
y2 = F2*math.sin(theta2r)
y_tot = y1 + y2
x1 = F1*math.cos(theta1r)
x2 = F2*math.cos(theta2r)
x_tot = x1 + x2
F_net = math.sqrt((y_tot**2)+(x_tot**2))
theta_net = math.atan(y_tot/x_tot)
theta_netd = math.degrees(theta_net)
if int(y_tot) <= 0:
    theta_netd += 180
#print magnitude and direction of net acceleration
a = F_net/m
print("The object moves at", a, "m/s at", theta_netd, "degrees")






