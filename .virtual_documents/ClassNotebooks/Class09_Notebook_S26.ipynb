











def f(x):
    func = x**4-2*x+1
    return func

b = 2
a = 0
N = 10
h=(b-a)/N
k_sum = 0
for k in range(1,N):
    val = f(a+k*h)
    k_sum += val

area = h*(0.5*f(a)+0.5*f(b)+k_sum)
print(area)


