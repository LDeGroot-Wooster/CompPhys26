def f(x):
    func = x**4-2*x+1
    return func

b = 2
a = 0
N = 10
h=(b-a)/N
k_odd_sum_1 = 0
for k in range(1,N,2):
    val = f(a+k*h)
    k_odd_sum_1 += val

k_even_sum_1 = 0
for k in range(2,N,2):
    val = f(a+k*h)
    k_even_sum_1 += val

I1 = (1/3)*h*(f(a)+f(b)+4*k_odd_sum_1+2*k_even_sum_1)
print(I1)

N = 20
h=(b-a)/N
k_odd_sum = 0
for k in range(1,N,2):
    val = f(a+k*h)
    k_odd_sum += val

k_even_sum = 0
for k in range(2,N,2):
    val = f(a+k*h)
    k_even_sum += val

I2 = (1/3)*h*(f(a)+f(b)+4*k_odd_sum+2*k_even_sum)
print(I2)





delta = (1/15)*(I2-I1)
print(delta)





