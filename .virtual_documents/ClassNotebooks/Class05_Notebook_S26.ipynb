t = np.linspace(1,10,15)

g = 9.81
h = 500 - (1/2)*g*(t**2)
plt.figure(1)
plt.plot(t,h,"bo")
plt.xlabel("time (s)")
plt.ylabel("height (m)")
plt.title("Height of object in free fall")
plt.grid()
z=t**2
plt.figure(2)
plt.plot(t,h,"bo")
plt.plot(t,z,"m:")
plt.show()











