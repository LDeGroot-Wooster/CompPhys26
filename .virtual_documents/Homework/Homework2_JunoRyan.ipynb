#set values
a_1 = 15.8*1000000   #in eV
a_2 = 18.3*1000000
a_3 = 0.714*1000000
a_4 = 23.2*1000000

#request inputs
Z = int(input("Input the atomic number of the atom"))

#store best values of B and A
max_B = 0
ideal_A = 0

for i in range(Z,3*Z+1): #3Z+1 so 3Z is included in the range
    A = i
    #determine a_5
    if A%2 != 0: #if A odd
        a_5 = 0
    elif A%2==0 and Z%2==0:  #if A and Z even
        a_5 = 12*1000000
    elif A%2==0 and Z%2!=0:   #if A even and Z odd
        a_5 = -12*1000000
    #calculate B. If larger than current max B, replace max B and record A
    B = (a_1*A)-(a_2*(A**(2/3)))-(a_3*(Z**2)/(A**(1/3)))-(a_4*((A-2*Z)**2)/A)+(a_5/(A**(1/2)))
    if B > max_B:
        max_B = B
        ideal_A = A
    
print("The binding energy per nucleon is about", round(B/A/1000000,2), "MeV at an atomic mass\
of", A, ", which is the most stable nucleus with an atomic number of", Z, ".")











def CtoF(C_temp):
    F_temp = [0] * len(C_temp)  #list of zeroes of same length as input list
    for i in range(len(C_temp)):
        F_temp[i] = (C_temp[i]*9/5)+32  #translates each temperature into fahrenheit
    return F_temp

print(CtoF([5, 10, 15, 20, 25, 30, 35, 40, 45, 50, 55, 60, 65, 70, 75]))














prime_list = [2]

def isPrime(n,primes):   #returns 0 if not prime, returns value if prime
        for j in primes:
            if j > np.sqrt(n):
                return n
            if n%j == 0:  
                return 0

#checks if output of function above is 0 for all values in range, if not, appends output to prime list
for i in range(3,10001):
    n = isPrime(i,prime_list)
    if n!=0:
        prime_list.append(n)

print(prime_list)











#plots sunspot data
data = np.loadtxt("sunspots.txt",float)
months = data[:1000,0]
spots = data[:1000,1]
plt.scatter(months,spots,s=6,c="g")
plt.xlabel("Months since January 1749")
plt.ylabel("Number of Sunspots")
plt.title("Number of Sunspots Observed on the Sun over Time")

#empty list to be filled with runnign average data
avg_spots = []
r=5
def run_avg(month):
    if month < r or month > 1000 - (r+1): #checks if running average possible. If not, returns 0
        return 0
    else:                           #performs running average calculation by summing five values
        total_spots = 0             #below and above, plus given value, and dividing by number
        for i in range(-r,r+1):     #of values
            total_spots+=spots[month+i]
        avg_spots = total_spots/(2*r+1)
        return avg_spots

#populates list and plots data
for month in range(len(months)):
    avg_spots.append(run_avg(month))
plt.plot(months,avg_spots)
plt.show














#calculates deltoid
x_vals = []
y_vals = []
for theta in np.linspace(0,2*np.pi,80):
    x=2*np.cos(theta)+np.cos(2*theta)
    y=2*np.sin(theta)-np.sin(2*theta)
    x_vals.append(x)
    y_vals.append(y)

#plots deltoid
plt.figure(1)
plt.scatter(x_vals,y_vals,s=6,c="g")
plt.show()

#calculate spiral
x_vals2 = []
y_vals2 = []
for theta in np.linspace(0,10*np.pi,200):
    r = theta**2
    x = r*np.cos(theta)
    y = r*np.sin(theta)
    x_vals2.append(x)
    y_vals2.append(y)

#plot spiral
plt.figure(2)
plt.scatter(x_vals2,y_vals2,s=6,c="g")
plt.show()

#calculate Fey
x_vals3 = []
y_vals3 = []
for theta in np.linspace(0,24*np.pi,4000):
    r = (math.e**(np.cos(theta)))-(2*np.cos(4*theta))+((np.sin(theta/12))**5)
    x = r*np.cos(theta)
    y = r*np.sin(theta)
    x_vals3.append(x)
    y_vals3.append(y)

#plot Fey
plt.figure(2)
plt.scatter(x_vals3,y_vals3,s=6,c="g")
plt.show()






