#define values
r_0 = 0.45 #meters
ep_0 = 8.854e-12 #Farads/meter
Q = 0.25e-9 #Coulombs
r_arr = np.linspace(0,3*r_0,100) #array of r values

#calculates E for non-conducting sphere
def non_cond_sphere(array):
    E_arr = np.zeros(len(r_arr)) #empty array to store
    for i in range(len(array)): #calculates E for each r value
        rad = array[i]
        if rad > r_0:
            E = (1/(4*np.pi*ep_0))*(Q/(rad**2))
        elif rad < r_0:
            E = (1/(4*np.pi*ep_0))*(Q*rad/(r_0**3))
        E_arr[i] = E
    return E_arr

#calculates E for conducting sphere
def cond_sphere(array):
    E_arr = np.zeros(len(r_arr)) #empty array to store
    for i in range(len(array)): #calculates E for each r value
        rad = array[i]
        if rad > r_0:
            E = (1/(4*np.pi*ep_0))*(Q/(rad**2))
        elif rad < r_0:
            E = 0
        E_arr[i] = E
    return E_arr

#randomize error
non_cond = non_cond_sphere(r_arr)
cond = cond_sphere(r_arr)
non_cond_err = np.zeros(len(r_arr))
for i in range(len(non_cond_err)):
    err = random.uniform(0.5,1.0)
    non_cond_err[i] = err
cond_err = np.zeros(len(r_arr))
for i in range(len(cond_err)):
    err = random.uniform(0.5,1.0)
    cond_err[i] = err

#plotting
plt.figure(1)
plt.scatter(r_arr,non_cond,s=16,c="b",label = "Non-conducting Sphere")
plt.errorbar(r_arr,non_cond,yerr=non_cond_err,fmt = "None", capsize = 2)
plt.scatter(r_arr,cond,s=8,c="r",label = "Conducting Sphere")
plt.errorbar(r_arr,cond,yerr=cond_err,fmt = "None", capsize = 2, c="r")
plt.xlabel("Radius (m)")
plt.ylabel("Electric Field Strength (N/C)")
plt.title("Electric Field Strength for Conducting and Non-conducting Sphere")
plt.legend(fontsize=12)
plt.show()











