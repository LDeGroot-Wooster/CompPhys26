def psi(x,y,z):
    r = np.sqrt(x**2+y**2+z**2)
    psi = z*(2-r)*(math.e**(-r))
    return psi

side = 4 #side length
num_points = 1000
spacing = side/num_points
probs = np.zeros([num_points,num_points], float)

#calculate psi probability density for each x and z
for i in range(num_points):
    x = i*spacing
    for j in range(num_points):
        y = j*spacing
        prob = np.abs(psi(x,0,z))**2
        probs[i,j] = prob

